{
  "$schema": "https://raw.githubusercontent.com/Microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "gam300-engine",
  "version": "1.0.0",
  "description": "GAM300 3D Game Engine - Cross-platform game engine with editor support",
  "homepage": "https://github.com/yyanorg/gam300-project",
  "license": "MIT",
  "dependencies": [
    {
      "name": "joltphysics",
      "default-features": false,
      "features": ["rtti", "debugrenderer", "profiler"]
    },
    "assimp",
    "glfw3",
    "freetype",
    "glad",
    "glm",
    "spdlog",
    "rapidjson",
    "gli",
    "meshoptimizer"
  ],
  "overrides": [
    {
      "name": "joltphysics",
      "version": "5.4.0"
    }
  ],
  "builtin-baseline": "2d6a6cf3ac9a7cc93942c3d289a2f9c661a6f4a7"
}