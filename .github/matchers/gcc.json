{
  "problemMatcher": [
    {
      "owner": "gcc-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\s+error:\\s+(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "gcc-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\s+warning:\\s+(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "linker-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+): undefined reference to `(.+)'",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    }
  ]
}
